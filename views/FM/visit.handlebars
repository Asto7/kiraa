<center><h3>{{fm.title}}</h3></center>
<center><span style="color:brown; font-size:25px;">Code:</span></center>
<pre   class="box main card card-body " id='kira'  style=" margin:5px auto; font-size: 15px;border-color: blue; float:center; width:80%;  height:80%; "><code>{{fm.code}}</code></pre>
{{#if fm.tags}}
<center><span style="color:brown; font-size:25px;">Problem Tags:</span></center>
<pre   class="box main card card-body " id='kira'  style=" margin:10px auto; font-size: 15px; border-color: blue; float:center; width:80%;  height:80%; "><code>{{#each fm.tags as |K|}}<span onclick="goto()" style='cursor:pointer; margin-right:1em; font-size:0.9em; font-family:`Times New Roman`, Times, serif; letter-spacing:0.1em;' class='badge badge-secondary'>{{K}}</span>{{/each}}</code></pre>
{{/if}}
<center><span style="color:brown; font-size:25px;">ReadMe:</span></center>
<pre   class="box main card card-body " id='kira'  style=" margin:10px auto; font-size: 15px; border-color: blue; float:center; width:80%;  height:80%; "><code>{{fm.details}}</code></pre>

<script>
    function goto(){
             window.open( 
              "https://www.google.co.in/search?q=geeksforgeeks+"+event.target.innerHTML, "_blank"); 
    }
</script>
