{{#each fm}}

  <div class="card card-body mb-2">
        <h4>{{title}}</h4>
        <p style="font-weight:700; letter-spacing:1px;">By: <span style="color:blue; letter-spacing:1.5px;">{{user}}</span></p>  
    <p>Creation date: {{creationDate}}</p>
    <a class="btn btn-dark btn-block mb-2" style='background-color:#123C69' href="/FM/visit/{{id}}">visit</a>
  </div>
{{else}}
  <p>No Code Made so far By the User</p>
{{/each}}
